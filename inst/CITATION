
year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "{Fusion}: an integrated dataset library for metabolic profiling",
         author = c(person("Julien", "Wist")),
         year = year,
         note = note,
         url = "https://github.com/jwist/fusion")